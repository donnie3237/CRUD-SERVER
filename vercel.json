{
    "version": 2,
    "builds": [
      {
        "src": "index.ts",
        "use": "npm",
        "config": {
          "command": "npm run build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/index.ts"
      }
    ]
  }