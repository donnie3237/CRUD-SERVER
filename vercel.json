{
    "version": 2,
    "builds": [
      {
        "src": "index.ts",
        "use": "@vercel/node",
        "config": {
          "env": {
            "TS_NODE_PROJECT": "tsconfig.json"
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.ts"
      }
    ]
  }