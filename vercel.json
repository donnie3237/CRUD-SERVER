{
    "version": 2,
    "builds": [
      {
        "src": "index.ts",
        "use": "pnpm",
        "config": {
          "command": "pnpm i && pnpm run vercel"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/index.ts"
      }
    ]
  }